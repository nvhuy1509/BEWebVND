    <script>
       
      function myFunction() {
        console.log('delete-----------------')
        const arrow1 = document.getElementById('arrow1');
        const arrow2 = document.getElementById('arrow2');
        const arrow3 = document.getElementById('arrow3');
        const arrow4 = document.getElementById('arrow4');
        arrow1.classList.remove('work-arrow-right');
        arrow2.classList.remove('work-arrow-left');
        arrow3.classList.remove('work-arrow-right');
        arrow4.classList.remove('work-arrow-left');
      }
      
      function myFunctionRestart() {
        const arrow1 = document.getElementById('arrow1');
        const arrow2 = document.getElementById('arrow2');
        const arrow3 = document.getElementById('arrow3');
        const arrow4 = document.getElementById('arrow4');
        arrow1.classList.add('work-arrow-right');
        arrow2.classList.add('work-arrow-left');
        arrow3.classList.add('work-arrow-right');
        arrow4.classList.add('work-arrow-left');
      }

      setInterval(myFunction, 8000);
      setInterval(myFunctionRestart, 10000);
    </script>
